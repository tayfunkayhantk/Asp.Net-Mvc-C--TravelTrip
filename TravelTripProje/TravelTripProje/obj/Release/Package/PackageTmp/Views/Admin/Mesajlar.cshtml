@using TravelTripProje.Models.Siniflar
@model List<Iletisim>

@{
    ViewBag.Title = "Mesajlar";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Mesajlar</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Ad Soyad</th>
        <th>Mail</th>
        <th>Telefon</th>
        <th>Mesaj</th>
        <th>Sil</th>

    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.AdSoyad</td>
            <td>@x.Mail</td>
            <td>@x.Telefon</td>
            <td>@x.Mesaj</td>
            <td><a href="/Admin/MesajSil/@x.ID" class="btn btn-danger">Sil</a></td>


        </tr>
    }
</table>
